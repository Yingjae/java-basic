package overriding;

public class Main01P265 {

	public static void main(String[] args) {
		// 말과 고래를 생성해보겠습니다.
		Horse h1 = new Horse();
		h1.howl();
		h1.name = "조랑말";
		h1.weight = 350;
		h1.habitat = "초원";
		h1.rank = 1;
		h1.getInfo();
		h1.getHorseInfo();
		
		Whale w1 = new Whale();
		w1.howl();

	}

}
